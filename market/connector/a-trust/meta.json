{
    "id": "atrust",
    "name": "A-Trust Mobile phone signature",
    "tags": ["connector"],
    "mavenArtifacts": [
        {
            "name": "A-Trust",
            "groupId": "ch.ivyteam.ivy.addons",
            "artifactId": "a-trust",
            "makesSenseAsMavenDependency": true
        },
        {
            "name": "A-Trust Demo",
            "groupId": "ch.ivyteam.ivy.addons",
            "artifactId": "a-trust-demos"
        }
    ],
    "installers": [
        {
            "id": "maven-dependency",
            "data": {
                "dependencies": [
                    {
                        "groupId": "ch.ivyteam.ivy.addons",
                        "artifactId": "a-trust",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories":[
                    {
                        "id":"axonivy.public",
                        "url":"http://repo.axonivy.rocks",
                        "snapshots":{"enabled":"true"}
                    }
                ]
            }
        },
        {
            "id": "maven-import",
            "data": {
                "projects":[
                    {
                        "groupId": "ch.ivyteam.ivy.addons",
                        "artifactId": "a-trust-demos",
                        "version": "${version}",
                        "type": "iar"
                    }
                ],
                "repositories":[
                    {
                        "id":"axonivy.public",
                        "url":"http://repo.axonivy.rocks"
                    }
                ]
            }
        },
        {
            "id": "open-resource",
            "data": {
                "project": "ATrustDemos",
                "resources": [
                    "processes/ATrustIntegrationDemo/DemoStart.mod"
                ]
            }
        }
    ]
}
